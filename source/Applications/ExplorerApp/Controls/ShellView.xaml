<UserControl x:Class="ExplorerApp.Controls.ShellView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dd="urn:gong-wpf-dragdrop"
             xmlns:local="clr-namespace:ExplorerApp.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Template="{DynamicResource ShellView}"
             AllowDrop="True"
             dd:DragDrop.IsDragSource="True"
             dd:DragDrop.IsDropTarget="True"
             
             dd:DragDrop.DropHandler="{x:Static helpers:DropHandler.Instnace}"
             xmlns:helpers="clr-namespace:ExplorerApp.Helpers">
    <Grid>
            
    </Grid>
</UserControl>
