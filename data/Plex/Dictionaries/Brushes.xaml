<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="DefaultThemeBrush" Color="#EBEBE4"/>

    <SolidColorBrush x:Key="ActiveWindowBackgroundBrush" Color="#3A6EA5"/>
    <SolidColorBrush x:Key="ActiveWindowBorderBrush" Color="#4D83BC"/>

    <SolidColorBrush x:Key="ButtonNormalBorderBrush" Color="#7F817B"/>
    <LinearGradientBrush x:Key="ButtonNormalInnerBorderBrush"
                         StartPoint="0,0" EndPoint="1,1" >
        <GradientStop Color="#FFFFFF"/>
        <GradientStop Color="#BCBEB8" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonNormalInnerBackgroundBrush"
                         StartPoint="0,0" EndPoint="0,1" >
        <GradientStop Color="#F0F2EC"/>
        <GradientStop Color="#DEE0DA" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonHoverInnerBorderBrush"
                         StartPoint="0,0" EndPoint="1,1" >
        <GradientStop Color="#C5CCE8"/>
        <GradientStop Color="#8B94B4" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonPressedInnerBorderBrush"
                         StartPoint="0,0" EndPoint="1,1" >
        <GradientStop Color="#8B94B4" />
        <GradientStop Color="#C5CCE8" Offset="1"/>
    </LinearGradientBrush>
</ResourceDictionary>